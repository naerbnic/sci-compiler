# Libraries for semantic analysis of SCI scripts.

package(
    default_visibility = ["//scic:scic_internal"],
)

cc_library(
    name = "symbol",
    srcs = ["symbol.cpp"],
    hdrs = ["symbol.hpp"],
    deps = [
        "//util:choice",
        "//util/strings:ref_str",
        "//util/types:strong_types",
    ],
)

cc_library(
    name = "module_env",
    srcs = ["module_env.cpp"],
    hdrs = ["module_env.hpp"],
    deps = [
        ":symbol",
        "//util/strings:ref_str",
    ],
)
